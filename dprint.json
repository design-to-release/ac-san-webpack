{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "typescript": {
    "quoteStyle": "preferSingle",
    "indentWidth": 2
  },
  "json": {
  },
  "markdown": {
  },
  "toml": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/dist"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.50.2.wasm",
    "https://plugins.dprint.dev/json-0.12.3.wasm",
    "https://plugins.dprint.dev/markdown-0.9.4.wasm",
    "https://plugins.dprint.dev/toml-0.4.1.wasm"
  ]
}
